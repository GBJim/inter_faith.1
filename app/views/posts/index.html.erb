<%- model_class = Post -%>
<div class="page-header ">
  <h1><%=t '.title', :default => Post %></h1>
</div>
<%= link_to t('.new', :default => t("helpers.links.new")),
            new_post_path,
            :class => 'btn btn-primary' %>

<div id = "my-posts" class="container-liquid waterfall">
  <%= render @posts%>
</div>
<div id="infinite-scrolling">
  <%= will_paginate %>
</div>
