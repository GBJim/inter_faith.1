<%- model_class = Post -%>
<div class="page-header ">
  <h1><%=t '.title', :default => Post %></h1>
</div>

<div class="container-liquid">
  <%= render partial: "post", collection: @posts %>
</div>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_post_path,
            :class => 'btn btn-primary' %>
